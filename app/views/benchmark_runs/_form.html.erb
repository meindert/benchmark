<%= simple_form_for @benchmark_run, :html => {:multipart => true} do |f| %>
    <%= f.association :benchmark_set %>
    <%= f.input :act_month, :as => :string %>
    <%= f.input :note, :as => :text, :input_html => {:rows => 5, :cols => 40} %>

    <%= file_field_tag :file %>
    <%= submit_tag "Import" %>



<% end %>

